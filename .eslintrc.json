{
  "extends": "airbnb-base",
  "rules": {
    "consistent-return": 0,
    "comma-dangle": 0,
    "space-unary-ops": 0,
    "no-mixed-operators": 0,
    "prefer-template": 0,
    "no-console": 0,
    "class-methods-use-this": 0,
    "no-param-reassign": 1,
    "no-unused-vars": 1,
    "no-await-in-loop": 1
  }
}